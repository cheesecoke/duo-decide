{
	"name": "duo-decide",
	"version": "1.0.0",
	"main": "expo-router/entry",
	"scripts": {
		"start": "expo start",
		"web": "expo start --web",
		"build": "expo export --platform web",
		"android": "expo run:android",
		"ios": "expo run:ios",
		"lint": "eslint . --fix",
		"generate-colors": "node ./scripts/generate-colors.js && eslint ./constants/colors.ts --fix",
		"test": "jest",
		"test:watch": "jest --watch",
		"test:coverage": "jest --coverage",
		"db:push": "supabase db push",
		"db:pull": "supabase db pull",
		"db:diff": "supabase db diff --use-migra -f",
		"db:reset": "supabase db reset",
		"prepare": "husky"
	},
	"dependencies": {
		"@emotion/native": "^11.11.0",
		"@emotion/react": "^11.14.0",
		"@emotion/styled": "^11.14.1",
		"@expo-google-fonts/outfit": "^0.4.3",
		"@expo-google-fonts/plus-jakarta-sans": "^0.4.2",
		"@hookform/resolvers": "^5.0.1",
		"@react-native-async-storage/async-storage": "^2.1.2",
		"@rn-primitives/label": "^1.1.0",
		"@rn-primitives/radio-group": "^1.1.0",
		"@rn-primitives/slot": "^1.1.0",
		"@rn-primitives/switch": "^1.1.0",
		"@rn-primitives/types": "^1.1.0",
		"@supabase/supabase-js": "^2.49.4",
		"aes-js": "^3.1.2",
		"claude": "^0.1.1",
		"expo": "^53.0.20",
		"expo-constants": "~17.1.6",
		"expo-dev-client": "~5.1.8",
		"expo-font": "~13.3.2",
		"expo-image": "~2.1.7",
		"expo-linking": "~7.1.4",
		"expo-router": "~5.0.7",
		"expo-secure-store": "~14.2.3",
		"expo-status-bar": "~2.2.3",
		"expo-system-ui": "~5.0.7",
		"phosphor-react-native": "^3.0.3",
		"react": "19.0.0",
		"react-dom": "19.0.0",
		"react-hook-form": "^7.55.0",
		"react-native": "0.79.2",
		"react-native-date-picker": "^5.0.13",
		"react-native-gesture-handler": "~2.24.0",
		"react-native-get-random-values": "~1.11.0",
		"react-native-reanimated": "~3.17.3",
		"react-native-safe-area-context": "5.4.0",
		"react-native-screens": "~4.10.0",
		"react-native-svg": "^15.12.1",
		"react-native-url-polyfill": "^2.0.0",
		"react-native-web": "^0.20.0",
		"zod": "^3.24.3"
	},
	"devDependencies": {
		"@babel/core": "^7.20.0",
		"@testing-library/react-native": "^13.3.3",
		"@types/aes-js": "^3.1.4",
		"@types/jest": "^30.0.0",
		"@types/react": "~19.0.10",
		"babel-plugin-module-resolver": "^5.0.2",
		"eslint": "^9.0.0",
		"eslint-config-expo": "~9.2.0",
		"eslint-config-prettier": "^10.1.2",
		"eslint-plugin-prettier": "^5.2.6",
		"husky": "^9.1.7",
		"jest": "^30.2.0",
		"jest-expo": "^54.0.17",
		"lint-staged": "^16.2.7",
		"prettier": "^3.6.2",
		"react-native-svg-transformer": "^1.5.3",
		"react-test-renderer": "^19.0.0",
		"typescript": "~5.8.3"
	},
	"expo": {
		"install": {
			"exclude": [
				"@supabase/supabase-js"
			]
		}
	},
	"private": true,
	"lint-staged": {
		"*.{ts,tsx,js,jsx,json,md,yml,yaml,css}": "prettier --write"
	}
}
